{
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "editor.formatOnPaste": true,
    "editor.fontSize": 16,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.fixAll.eslint": false
    },
    "editor.maxTokenizationLineLength": 20000,
    "git.ignoreLimitWarning": true,
    "cSpell.enableFiletypes": [
        "ansible",
        "ansible-advanced",
        "bat",
        "clojure",
        "coffeescript",
        "dart",
        "diff",
        "dockerfile",
        "editorconfig",
        "fsharp",
        "git-rebase",
        "groovy",
        "hlsl",
        "ignore",
        "ini",
        "jsx-tags",
        "log",
        "lua",
        "makefile",
        "objective-c",
        "objective-cpp",
        "perl",
        "perl6",
        "powershell",
        "properties",
        "r",
        "razor",
        "ruby",
        "scminput",
        "search-result",
        "shaderlab",
        "shellscript",
        "sql",
        "swift",
        "vb",
        "xml",
        "xsl"
    ]
}